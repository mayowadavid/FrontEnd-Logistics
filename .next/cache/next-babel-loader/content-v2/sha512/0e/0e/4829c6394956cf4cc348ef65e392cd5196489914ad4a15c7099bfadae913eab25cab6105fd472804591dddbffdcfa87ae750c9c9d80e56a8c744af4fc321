n\r\n    const handleSignupSubmit = async(e) => {\r\n        e.preventDefault();\r\n        const res = await axios.post('/admin/signup', signup);\r\n            console.log(res)\r\n      }; \r\n\r\n      const handleLoginSubmit = async(e) => {\r\n        e.preventDefault();\r\n        setAuthenticating(true);\r\n        const res = await axios.post('/signin', login);\r\n        if(res.status === 200){\r\n            setAuthenticating(false);\r\n            setAuthenticate(true);\r\n            const {token, user}= res.data;\r\n            localStorage.setItem('token', token);\r\n            let message = \"welcome\";\r\n            setSuccess({...success, message, token, user});\r\n            return console.log(success);\r\n        }\r\n      }; \r\n\r\n\r\n\r\n    return (\r\n        <AuthContext.Provider value={{login, signup, handleLoginChange, handleSignupChange, handleSignupSubmit, handleLoginSubmit, error }}>\r\n            {props.children}\r\n        </AuthContext.Provider>\r\n    );\r\n}\r\n\r\nexport default AuthContextProvider;\r\n"]},"metadata":{},"sourceType":"module"}