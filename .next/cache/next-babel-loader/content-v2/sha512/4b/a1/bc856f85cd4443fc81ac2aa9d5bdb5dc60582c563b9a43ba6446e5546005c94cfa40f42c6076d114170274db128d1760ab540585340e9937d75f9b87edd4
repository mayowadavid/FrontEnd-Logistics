           const {name, value} = e.target;\r\n            sender[name] = e.target.value;\r\n            const newInput = { ...input, sender, [name]: value };\r\n            return setInput(newInput);\r\n         }\r\n     }; \r\n        const handleReceiver = (e) => {\r\n            \r\n                if(e){\r\n                    const {receiver} = {...input};\r\n                    const {name} = e.target;\r\n                    receiver[name] = e.target.value;\r\n                    const newInp = { ...input, receiver };\r\n                    return setInput(newInp);\r\n                }\r\n            }; \r\n\r\n    const handleFormSubmit = (e) => {\r\n        e.preventDefault();\r\n        return setCount(1);\r\n\r\n    }\r\n    const {galleryImages} = {...input}\r\n\r\n    const photoChange = (e) => {\r\n        \r\n        let photoFile = e.target.files;\r\n\r\n         let Format = ['image/jpeg', 'image/png', 'image/gif', 5000000];\r\n\r\n         let index = Array.from(photoFile).filter(x=> (x.type == Format[0] || x.type == Format[1]) && (x.size <= Format[2] || x.size <= Format[3]) );\r\n            let {name} = e.target;\r\n            \r\n         \r\n         if(index){\r\n             console.log(input)\r\n                const photoUrl = Array.from(index).map((file)=> URL.createObjectURL(file));\r\n                galleryImages[name] = galleryImages[name].concat(photoUrl);\r\n                let newSource = {...input, galleryImages }\r\n                setInput(newSource);\r\n                 Array.from(index).map((file)=> URL.revokeObjectURL(file));                                                 \r\n      }\r\n    }\r\n\r\n    const photoSource = (url) => {\r\n        return url.map((file) =>\r\n            <img src={file} key={uuidv4()}/>)\r\n    } \r\n    \r\n   \r\n   \r\n    \r\n    return (\r\n          <>\r\n              <div className=\"photo\">\r\n              \r\n                    { input.galleryImages.sourceUrl !== null ? (<form> \r\n